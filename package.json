{
  "name": "innei-tools",
  "private": true,
  "version": "0.9.1",
  "license": "MIT",
  "author": "Innei",
  "scripts": {
    "postinstall": "husky install",
    "release": "esmo scripts/release.ts",
    "publish": "zx scripts/publish.mjs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": [
      "prettier --ignore-path ./.prettierignore --write "
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint "
    ]
  },
  "eslintConfig": {
    "extends": [
      "@innei/eslint-config-ts"
    ]
  },
  "devDependencies": {
    "@innei/eslint-config-ts": "workspace:*",
    "@innei/prettier": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/prompts": "^2.0.14",
    "@types/semver": "^7.3.9",
    "consola": "2.15.3",
    "esno": "^0.14.1",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "prettier": "2.5.1",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "typescript": "4.5.5",
    "zx": "6.0.7"
  }
}
